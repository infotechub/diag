@model IEnumerable<NovoDiagnosis.Models.User>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PasswordHash)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PasswordSalt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CurrentEncryption)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastLoginDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoginAttempts)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ResetPasswordGuid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ResetPasswordExpiry)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DisableNotifications)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastNotificationReadDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Uiculture)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mobilephone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CugMobilephone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dob)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Guid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedOn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UpdatedOn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsDeleted)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PasswordHash)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PasswordSalt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CurrentEncryption)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastLoginDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoginAttempts)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ResetPasswordGuid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ResetPasswordExpiry)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DisableNotifications)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastNotificationReadDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Uiculture)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mobilephone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CugMobilephone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dob)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Guid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedOn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdatedOn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsDeleted)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
